msgid "MODIFY FIELD ABOVE BEFORE RE-IMPORTING"
msgstr "MODIFIQE EL CAMPO ARRIBA ANTES DE IMPORTAR"

#: ../../../JWLManager.py:1030
msgid ""
"LEAVE {TITLE=} (empty) IF YOU DON'T WANT TO DELETE ANY NOTES WHILE "
"IMPORTING\n"
msgstr "DEJE {TITLE=} (vacío) SI NO DESEA ELIMINAR NINGUNA NOTA AL IMPORTAR\n"

#: ../../../JWLManager.py:1031
msgid "EACH NOTE STARTS WITH HEADER INDICATING CATEGORY, ETC."
msgstr "CADA NOTA COMIENZA CON UN ENCABEZAMIENTO DE CATEGORÍA, ETC."

#: ../../../JWLManager.py:1032
msgid "BE CAREFUL WHEN MODIFYING THE ATTRIBUTES\n"
msgstr "TENGA CUIDADO AL MODIFICAR LOS ATRIBUTOS\n"

#: ../../../JWLManager.py:1033
msgid "LINE AFTER HEADER IS NOTE TITLE"
msgstr "LA LÍNEA DESPUÉS DEL ENCABEZAMIENTO ES TÍTULO DE LA NOTA"

#: ../../../JWLManager.py:1034
msgid ""
"REST IS NOTE BODY; CAN BE MULTI-LINE AND IS TERMINATED BY NEXT NOTE "
"HEADER\n"
msgstr ""
"EL RESTO ES EL CUERPO; PUEDE SER DE VARIAS LÍNEAS Y ES TERMINADO POR OTRO"
" ENCABEZAMIENTO\n"

#: ../../../JWLManager.py:1035
msgid "SEPARATE TAGS WITH \",\" (commas)"
msgstr "SEPARE ETIQUETAS CON \",\" (coma)"

#: ../../../JWLManager.py:1036
msgid "OR LEAVE EMPTY IF NO TAG: {TAGS=bible,notes} OR {TAGS=}\n"
msgstr "O DEJE VACÍO (SIN ETIQUETA): {TAGS=biblia,notas} O {TAGS=}\n"

#: ../../../JWLManager.py:1037
msgid "FILE SHOULD TERMINATE WITH \"==={END}===\"\n"
msgstr "EL ARCHIVO DEBE TERMINAR CON \"==={END}===\"\n"

#: ../../../JWLManager.py:1038
msgid "ENSURE YOUR FILE IS ENCODED AS UTF-8 (UNICODE)"
msgstr "ASEGÚRESE QUE EL ARCHIVO ESTÉ EN FORMATO UTF-8 (UNICODE)"

