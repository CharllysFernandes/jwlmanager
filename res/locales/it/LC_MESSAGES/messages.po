msgid "MODIFY FIELD ABOVE BEFORE RE-IMPORTING"
msgstr "MODIFICA IL CAMPO SOPRA PRIMA DI RE-IMPORTARE"

#: ../../../JWLManager.py:1030
msgid ""
"LEAVE {TITLE=} (empty) IF YOU DON'T WANT TO DELETE ANY NOTES WHILE "
"IMPORTING\n"
msgstr ""
"LASCIA {TITLE=} (vuoto) SE NON VUOI ELIMINARE NESSUNA NOTA DURANTE "
"L'IMPORTAZIONE\n"

#: ../../../JWLManager.py:1031
msgid "EACH NOTE STARTS WITH HEADER INDICATING CATEGORY, ETC."
msgstr "TUTTE LE NOTE INIZIANO CON UN'INTESTAZIONE INDICANTE LA CATEGORIA, ETC."

#: ../../../JWLManager.py:1032
msgid "BE CAREFUL WHEN MODIFYING THE ATTRIBUTES\n"
msgstr "PRESTARE ATTENZIONE QUANDO SI MODIFICANO GLI ATTRIBUTI\n"

#: ../../../JWLManager.py:1033
msgid "LINE AFTER HEADER IS NOTE TITLE"
msgstr "LA LINEA DOPO L'INTESTAZIONE È IL TITOLO DELLA NOTA"

#: ../../../JWLManager.py:1034
msgid ""
"REST IS NOTE BODY; CAN BE MULTI-LINE AND IS TERMINATED BY NEXT NOTE "
"HEADER\n"
msgstr ""
"IL RESTO È IL CORPO DELLA NOTA; PUò ESSERE MULTI-LINEA ED È TERMINATO "
"DALL'INTESTAZIONE DELLA NOTA SUCCESSIVA\n"

#: ../../../JWLManager.py:1035
msgid "SEPARATE TAGS WITH \",\" (commas)"
msgstr "SEPARA I TAG CON \",\" (virgole)"

#: ../../../JWLManager.py:1036
msgid "OR LEAVE EMPTY IF NO TAG: {TAGS=bible,notes} OR {TAGS=}\n"
msgstr "O LASCIA VUOTO SE NESSUN TAG: {TAGS=bible,notes} O {TAGS=}\n"

#: ../../../JWLManager.py:1037
msgid "FILE SHOULD TERMINATE WITH \"==={END}===\"\n"
msgstr "IL FILE DOVREBBE TERMINARE CON \"==={END}===\"\n"

#: ../../../JWLManager.py:1038
msgid "ENSURE YOUR FILE IS ENCODED AS UTF-8 (UNICODE)"
msgstr "ASSICURATI CHE IL TUO FILE SIA CODIFICATO COME UTF-8 (UNICODE)"

